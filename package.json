{
  "name": "webpack-features",
  "version": "3.4.6",
  "description": "Feature-based webpack configurator with built-in React support",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "jsnext:main": "dist/index.js",
  "author": "Aleksandr Snov <snov.contact@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch --coverage=false",
    "lint": "eslint src",
    "types": "flow",
    "prettify": "yarn run prettier --write \"{src,scripts}/**/*.js\"",
    "build": "rimraf dist/ && babel src --out-dir dist --ignore fixtures,__tests__,**/*.test.js,**/*.spec.js && yarn add-flow-types",
    "add-flow-types": "node scripts/addFlowTypes.js",
    "prepublishOnly": "yarn prettify && yarn lint && yarn test && yarn build"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "jest": {
    "rootDir": "",
    "testRegex": "(src).*((\\.|/)(test|spec))\\.jsx?$",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/dist/**",
      "!**/.coverage/**",
      "!**/scripts/**"
    ],
    "coverageDirectory": ".coverage"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.38",
    "cross-env": "^5.0.5",
    "eslint": "^4.15.0",
    "eslint-config-dqnt": "^2.1.0",
    "flow-bin": "^0.62.0",
    "jest": "^22.1.3",
    "prettier": "^1.7.4",
    "rimraf": "^2.6.1"
  },
  "peerDependencies": {},
  "dependencies": {
    "@babel/core": "^7.0.0-beta.38",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.38",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.38",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.38",
    "@babel/plugin-transform-classes": "^7.0.0-beta.38",
    "@babel/plugin-transform-react-jsx-source": "^7.0.0-beta.38",
    "@babel/polyfill": "^7.0.0-beta.38",
    "@babel/preset-env": "^7.0.0-beta.38",
    "@babel/preset-flow": "^7.0.0-beta.38",
    "@babel/preset-react": "^7.0.0-beta.38",
    "autoprefixer": "^7.1.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^22.1.0",
    "babel-loader": "8.0.0-beta.0",
    "babel-plugin-emotion": "^8.0.12",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "eslint-loader": "^1.9.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "node-sass": "^4.6.0",
    "postcss": "^6.0.13",
    "postcss-loader": "^2.0.8",
    "precss": "^2.0.0",
    "react-hot-loader": "^3.1.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.6.0"
  },
  "optionalDependencies": {
    "emotion": "^8.0.10",
    "react": "^16.1.1",
    "react-dom": "^16.1.1"
  }
}
